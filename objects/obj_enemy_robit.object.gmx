            <string>
var insight = 220;
var toNear = 100;

// Wenn Robit von Meele getroffen soll er Bewegungsunf√§hig sein 3 Sekunden
if (stunned == 1) {
    robitSpeed = 0;
    canShoot = 0;
}
else {
    robitSpeed = 4;
    canShoot = 1;
}

// Berechnung der Distanz zum Spieler
distanceX = self.x - obj_player.x; // X
distanceY = self.y - obj_player.y; // Y

// Wenn bei den Distanze-Werten eine negative Zahl entsteht, dann ,achen wir sie Positiv
if (distanceX &lt; 0) {
    distanceX = distanceX * -1;
}

if (distanceY &lt; 0) {
    distanceY = distanceY * -1;
}

// Solange Spieler weiter als Sichtweite entfernt, tue nix
if ((distanceX &lt; insight) and (distanceY &lt; insight)) {
    move_towards_point(obj_player.x,obj_player.y,robitSpeed);
}

// Ist der Spieler zu nah, dann soll der Roboter auf Abstand gehen
if ((distanceX &lt;= toNear) and (distanceY &lt;= toNear)) {
    
    if (self.x &lt; obj_player.x) {
        move_towards_point(obj_player.x-100,obj_player.y,robitSpeed);
    }
    else {
        move_towards_point(obj_player.x+100,obj_player.y,robitSpeed);
    }
    
    if (self.y &lt; obj_player.y) {
        move_towards_point(obj_player.x,obj_player.y-100,robitSpeed);
    }
    else {
        move_towards_point(obj_player.x,obj_player.y+100,robitSpeed);
    }
    
}
else {
    speed = robitSpeed - 2;
}

// Richtungsbestimmung
if (hspeed &gt; 0) image_xscale = 1;
else image_xscale = -1; 


