        <arguments>
          <argument>
            <kind>1</kind>
            <string>{
    
    mouseX = mouse_x;
    mouseY = mouse_y;

    
    // --- Steuerung --- //
    
    // Up
    if (keyboard_check(vk_up) or keyboard_check(ord('W'))) {
        // Neue Steuerung
        vspeed -= 3;
        if (vspeed &lt; -7) {
            vspeed = -7;
        }
    }
        
    // down    
    if (keyboard_check(vk_down) or keyboard_check(ord('S'))) {
        
        vspeed += 3;
        if (vspeed &gt; 7) {
            vspeed = 7;
        }
    }
     
    
    // Left
    if (keyboard_check(vk_left) or keyboard_check(ord('A'))) {
       
        hspeed -= 3;
        if (hspeed &lt; -7) {
            hspeed = -7;
        }
    }
    
    // Right
    if (keyboard_check(vk_right) or keyboard_check(ord('D'))) {
        
        hspeed += 3;
        if (hspeed &gt; 7) {
            hspeed = 7;
        }
    }
    
    
    // --- spirte mirroring --- //
    if (mouse_x &lt; self.x) image_xscale = -1;
    if (mouse_x &gt; self.x) image_xscale = 1;
    
    
    // --- Animation --- ///
    
    // While movement = true --&gt; player is walking    
    if ((vspeed != 0) or (hspeed != 0)) {
        sprite_index = spr_player_02;
        image_index += 1;
        image_speed = 1;
    }
        
    // Wenn Bewegung stopt = Player not moving
    if ((hspeed == 0) and (vspeed ==0)) {
        sprite_index = spr_player_01;
        image_index += 1;
        image_speed = 1;
    }
         
    
    
    // --- Viewport Settings --- //
    
    
    
    hereX = obj_player.x;
    hereY = obj_player.y;
    
    mouseThereX = mouse_x;
    mouseThereY = mouse_y;
        
    distanceX = mouseThereX - hereX;
    distanceY = mouseThereY - hereY;
    
    
    view_xview = x - view_wview / 2 + random(shake)- shake / 2 + (distanceX /4);
    view_yview = y - view_hview / 2 + random(shake)- shake / 2 + (distanceY /4);
   
    
    //show_debug_message(string(global.killCount));
    
    
// horizontal collision Blockade Blocks
if (place_meeting(x + hspeed, y, obj_lvl_blockade)){
    while(!place_meeting(x+sign(hspeed), y, obj_lvl_blockade)){
        x += sign(hspeed);
    }
    hspeed = 0;
}

//Vertical collision Blockade Blocks
if (place_meeting(x, y + vspeed, obj_lvl_blockade)){
    while(!place_meeting(x, y+sign(vspeed), obj_lvl_blockade)){
        y += sign(vspeed);
    }
    vspeed = 0;
}

// === Collision with Barrel === //

//horizontal
if (place_meeting(x + hspeed, y, obj_lvl_desert_rustedBarrel)){
    while(!place_meeting(x+sign(hspeed), y, obj_lvl_desert_rustedBarrel)){
        x += sign(hspeed);
    }
    hspeed = 0;
}

//Vertical 
if (place_meeting(x, y + vspeed, obj_lvl_desert_rustedBarrel)){
    while(!place_meeting(x, y+sign(vspeed), obj_lvl_desert_rustedBarrel)){
        y += sign(vspeed);
    }
    vspeed = 0;
}

    
    
    
}
</string>
