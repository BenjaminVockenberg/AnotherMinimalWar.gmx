            <string>// Creating the Level Ini

// savedate.ini wird geöffnet oder erstellt, falls nicht vorhanden
// befindlich im Ordner AppData/Local/AnotherMinimalWar
ini_open("savedata.ini");

// Highscor wird gelesen, falls vorhanden
global.highest = ini_read_real("highestScore", "top_score", 0.0);

// Getötete Feinde werden ausgelesen
statistic_killcount = ini_read_real("statistics","killcount",0.0);

// Achivemt auslesen : 1k killed enemies
achive_Killer1k = ini_read_real("achivements","achivement_Killer1k", 0.0);

// Es wird ermittelt, ob es eine höhere Score gab oder eine neue erreicht wurde
if global.highest &lt; global.score {
    // Wenn die errreichte Punktzahl höher is als die gespeicherte
    // Wird die neue als höchste Punktzahl gespeichert
    ini_write_real("highestScore", "top_score", global.score);
    
}
else {
    // Wenn die erspielte aktelle Punktuahl höher, wird diese normal gespeichert
    ini_write_real("latestScore", "score", global.score);
}

// Achivement 1k killed enemies
if ((statistic_killcount &gt;= 1000.00)) {
    ini_write_real("achivements","achivement_Killer1k",1.0);
    global.achive1kKilled = 1;
}
else {
     ini_write_real("achivements","achivement_Killer1k",0.0);
    global.achive1kKilled = 0;
}

// Killcount wird aktualisiert
ini_write_real("statistics","killcount",statistic_killcount + global.killCount);

// ini-Datei wird geschrieben und abgeschlossen
ini_close(); 
</string>
